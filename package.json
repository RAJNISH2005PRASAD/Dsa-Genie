{
  "name": "dsa-genie",
  "version": "1.0.0",
  "description": "AI-Powered Personalized DSA Practice Platform with Streaks, Coins & Contests",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install",
    "start": "cd server && npm start",
    "deploy-setup": "node deploy-setup.js",
    "deploy:backend": "echo 'Deploy backend to Render: https://render.com'",
    "deploy:frontend": "echo 'Deploy frontend to Vercel: https://vercel.com'"
  },
  "keywords": ["dsa", "ai", "learning", "gamification", "leetcode", "algorithms"],
  "author": "DSA Genie Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
} 